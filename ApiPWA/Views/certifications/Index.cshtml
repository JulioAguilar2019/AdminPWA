@model IEnumerable<ApiPWA.certifications>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cert_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cert_institution)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cert_description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cert_link)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.users.user_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cert_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cert_institution)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cert_description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cert_link)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.users.user_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.cert_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.cert_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.cert_id })
        </td>
    </tr>
}

</table>
